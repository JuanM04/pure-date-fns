  "version": "0.1.0",
  "author": {
    "name": "JuanM04",
    "email": "me@juanm04.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JuanM04/pure-date-fns.git"
  },
  "packageManager": "yarn@3.1.1",
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.17",
    "npm": "8.1.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
